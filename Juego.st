Object subclass: #Juego	instanceVariableNames: 'palo masodecartas'	classVariableNames: ''	poolDictionaries: ''	category: 'Juego'!!Juego methodsFor: 'as yet unclassified' stamp: 'Anonymous 11/15/2019 16:37'!palo|r a|r:=Random new.a:=#('COPA' 'ORO' 'BASTO' 'ESPADA').palo:= a atRandom.^palo.! !!Juego methodsFor: 'as yet unclassified' stamp: 'Anonymous 11/15/2019 16:38'!masoDeCartasmasodecartas  isNil ifTrue:[masodecartas :=OrderedCollection new].^masodecartas! !!Juego methodsFor: 'as yet unclassified' stamp: 'Anonymous 11/15/2019 16:46'!nroazar|r c valor|r:= Random new.c:=#(1 2 3 4 5 6 7 10 11 12).valor:=c atRandom .^valor asString.! !!Juego methodsFor: 'as yet unclassified' stamp: 'Anonymous 11/15/2019 16:58'!baraja|baraja|baraja:= self masoDeCartas.1 to: 40 do:[:i|	baraja add: (self nroazar ,' ',self palo)].(baraja asSet)sorted:[:i :j| j>i].Transcript clear.Transcript show:baraja.! !